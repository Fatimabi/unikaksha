    #Budget
    A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
P=A*350.34
Q=B*230.90
R=C*190.55
S=D*125.30
T=E*180.90
X=P+Q+R+S+T
if X<=15000:
    print("Yes")
else:
    print("No")
    
    #Sece Dining
    x=input()
y=int(input())
if x=="front" and y==1 or x=="rear" and y==2:
    print("Left Handed")
elif x=="front" and y==2 or x=="rear" and y==1:
    print("Right Handed")
else:
    print("Invalid input")
    
    #Intersection of a Circle
    import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
d=math.sqrt(((x1-x2)**2)+((y1-y2)**2))
if d==r1+r2:
    print("The circles are tangential")
elif d<=r1+r2:
    print("The circles overlap")
else:
    print("The circles are not overlap")
    
    #Microwave Oven
    print("Enter the number of items")
X=int(input())
print("Enter the single item heating time")
Y=float(input())
Z=Y+(50/100)*Y
R=Y+2*Y
if X<=2:
    print("The recommended heating time is %.2f"%Z)
elif X==3:
    print("The recommended heating time is %.2f"%R)
else:
    print("Number of items is more")
    
